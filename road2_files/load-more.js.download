var t_prlx_scrol = 0;
var t_prlx_prev_scrol = 0;
var delta = 100;

jQuery(window).scroll(function(e){
if(jQuery(window).width() > 800 && jQuery('.free-temp-image').length > 0 )
  {
    parallax();
  }
});

function parallax(){
  var scrolled = jQuery(window).scrollTop();
  var divnum = 0;
  if (t_prlx_prev_scrol + delta < scrolled)
  {
    t_prlx_prev_scrol = scrolled;
    return false;
  }

  jQuery('.free-temp-image').each(function(){
      jQuery(this).addClass('cus_parlox'+divnum);
      divnum++;
  });
  if (scrolled > t_prlx_prev_scrol)
  {
    t_prlx_prev_scrol = scrolled;
    var numItems = jQuery('.cus_parlox'+t_prlx_scrol).length;
    if (numItems > 0)
    {
      var x = jQuery('.cus_parlox'+t_prlx_scrol).offset().top;
      var div_bottom_pos = x + jQuery('.cus_parlox'+t_prlx_scrol).outerHeight();
      var scrolled_prlx = scrolled-x+46;
      var cond = jQuery('.cus_parlox'+t_prlx_scrol+' .free-temp-details').offset().top + jQuery('.cus_parlox'+t_prlx_scrol+' .free-temp-details').outerHeight() + 46;
      if (scrolled > x && cond < div_bottom_pos)
      {
        jQuery('.cus_parlox'+t_prlx_scrol+' .free-temp-details').css('top',+(scrolled_prlx)+'px');
      }
      var next_t_prlx_scrol = t_prlx_scrol+1; 
      if (jQuery('.cus_parlox'+next_t_prlx_scrol).length > 0)
      {
        var y = jQuery('.cus_parlox'+next_t_prlx_scrol).offset().top;
        if (y < scrolled && t_prlx_scrol < divnum)
        { 
          t_prlx_scrol++;
        }
      }
    }
  }
  else
  {
    t_prlx_prev_scrol = scrolled;
    var numItems = jQuery('.cus_parlox'+t_prlx_scrol).length;
    if (numItems > 0)
    {
      var x = jQuery('.cus_parlox'+t_prlx_scrol).offset().top;
      var div_bottom_pos =  jQuery('.cus_parlox'+t_prlx_scrol).outerHeight();
      var scrolled_prlx = scrolled-x + 46;
      var scrol_sec = scrolled_prlx + jQuery('.cus_parlox'+t_prlx_scrol+' .free-temp-details').outerHeight() + 46;
      if (scrolled > x && scrol_sec < div_bottom_pos)
      {
        jQuery('.cus_parlox'+t_prlx_scrol+' .free-temp-details').css('top',+(scrolled_prlx)+'px');
      }
     
      var next_t_prlx_scrol = t_prlx_scrol-1; 
      if (jQuery('.cus_parlox'+next_t_prlx_scrol).length > 0)
      {
        var y = jQuery('.cus_parlox'+next_t_prlx_scrol).offset().top + jQuery(window).height() + 50;
        if (y > scrolled && t_prlx_scrol > 0)
        { 
          t_prlx_scrol--;
        }
      }
    }
  }
}

  function validateEmail(email) {
      var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
      return re.test(email);
  }
