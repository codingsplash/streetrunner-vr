jQuery(window).scroll( function(){ 
	var scrltop = jQuery(window).scrollTop();
	if (scrltop >= 600) {
        jQuery(".fix-btn-sec").addClass("left-btns-fixed");
    } 
    else {
       jQuery(".fix-btn-sec").removeClass("left-btns-fixed");
    }
    if (jQuery('.post-container').length > 0 && scrltop + 200 >= jQuery(".post-container").offset().top){ 
        jQuery(".fix-btn-sec").removeClass("left-btns-fixed");
    }
    if (scrltop > 1200)
    {
      jQuery(".scroll-button").show();
    }
    else
    {
      jQuery(".scroll-button").hide();
    }
});



